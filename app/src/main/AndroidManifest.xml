<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="freeman.rx.gxj.com.freeman.activity">

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name="freeman.rx.gxj.com.freeman.parent.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:launchMode="singleTask"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".SettingActivity" />
        <activity android:name=".QQTabActivity" />
        <activity android:name="freeman.rx.gxj.com.freeman.recycleractivity.PullToRefreshActivity"/>
        <activity android:name="freeman.rx.gxj.com.freeman.recycleractivity.GridPullToRefreshActivity"/>
        <activity android:name="freeman.rx.gxj.com.freeman.recycleractivity.PullToRefreshExpandActivity"/>

        <activity android:name="freeman.rx.gxj.com.freeman.recycleractivity.TreeAdapterViewActivity"/>

        <activity android:name="freeman.rx.gxj.com.freeman.drag.LinearDragActivity"/>
        <activity android:name="freeman.rx.gxj.com.freeman.drag.CustomDragActivity"/>
        <activity android:name="freeman.rx.gxj.com.freeman.drag.GridDragActivity"/>
        <activity android:name="freeman.rx.gxj.com.freeman.drag.DynamicDragActivity"/>


        <activity android:name="freeman.rx.gxj.com.freeman.activity.SelectPictureActivity"/>
        <activity android:name="freeman.rx.gxj.com.freeman.activity.PhotoBrowserActivity"/>

        <activity android:name="freeman.rx.gxj.com.freeman.activity.NotificationUpdateActivity"/>

        <service android:name="freeman.rx.gxj.com.freeman.services.UpdateAppService"/>

        <activity
            android:name="product.finance.xmqq.com.livedetectlibrary.XMActivity"
            />
        <service android:name="freeman.rx.gxj.com.freeman.services.DownloadService" />
    </application>

</manifest>